#!/usr/bin/env radian
# default program for radian executable testing
# put stuff here when you are trying it out
# migrate things to the validation suite once they are supposed to work

import string from radian
import number from radian

sync io.print("Hello, world")
var foo = string.blank
for i in number.range(1, 10):
	foo = foo & " " & string.decimal(i)
end i
sync io.print(foo)

